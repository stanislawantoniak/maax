        <ZolagoOs_Pwr>
            <version>0.0.1</version>
        </ZolagoOs_Pwr>
    </modules>
    <global>
        <models>
            <zospwr>
                <class>ZolagoOs_Pwr_Model</class>
                <resourceModel>zospwr_resource</resourceModel>
            </zospwr>
