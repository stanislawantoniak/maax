                <class>ZolagoOs_Pwr_Model_Resource</class>
                <entities>
                    <point>
                        <table>zospwr_point</table>
                    </point>
                </entities>
            </zospwr_resource>
        </models>
        <blocks>
            <zospwr>
                <class>ZolagoOs_Pwr_Block</class>
            </zospwr>
        </blocks>
        <helpers>
            <zospwr>
                <class>ZolagoOs_Pwr_Helper</class>
            </zospwr>
        </helpers>
        <resources>
            <zospwr_setup>
                <setup>
                    <module>ZolagoOs_Pwr</module>
                </setup>
            </zospwr_setup>
        </resources>
    </global>
</config>