<?xml version="1.0"?>
<!--
/**
  
 */

-->
<config>
    <modules>
        <ZolagoOs_OmniChannelShippingClass>
            <version>1.1.4</version>
        </ZolagoOs_OmniChannelShippingClass>
    </modules>

    <global>
        <models>
            <udshipclass>
                <class>ZolagoOs_OmniChannelShippingClass_Model</class>
                <resourceModel>udshipclass_mysql4</resourceModel>
            </udshipclass>
            <udshipclass_mysql4>
                <class>ZolagoOs_OmniChannelShippingClass_Model_Mysql4</class>
                <entities>
                    <customer><table>udshipclass_customer</table></customer>
                    <customer_row><table>udshipclass_customer_row</table></customer_row>
                    <vendor><table>udshipclass_vendor</table></vendor>
                    <vendor_row><table>udshipclass_vendor_row</table></vendor_row>
                </entities>
            </udshipclass_mysql4>
        </models>

        <resources>
            <udshipclass_setup>
                <setup>
                    <module>ZolagoOs_OmniChannelShippingClass</module>
                    <class>Mage_Sales_Model_Mysql4_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </udshipclass_setup>
            <udshipclass_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </udshipclass_write>
            <udshipclass_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </udshipclass_read>
        </resources>

        <blocks>
            <udshipclass>
                <class>ZolagoOs_OmniChannelShippingClass_Block</class>
            </udshipclass>
        </blocks>

        <helpers>
            <udshipclass>
                <class>ZolagoOs_OmniChannelShippingClass_Helper</class>
            </udshipclass>
        </helpers>

        <udropship>
            <vendor>
                <fields>
                </fields>
            </vendor>
        </udropship>

        <template>
            <email>
            </email>
        </template>

        <events>
            <udropship_shipping_load_after>
                <observers>
                    <udshipclass>
                        <type>singleton</type>
                        <class>udshipclass/observer</class>
                        <method>udropship_shipping_load_after</method>
                    </udshipclass>
                </observers>
            </udropship_shipping_load_after>
            <udropship_shipping_save_after>
                <observers>
                    <udshipclass>
                        <type>singleton</type>
                        <class>udshipclass/observer</class>
                        <method>udropship_shipping_save_after</method>
                    </udshipclass>
                </observers>
            </udropship_shipping_save_after>
            <udropship_shipping_save_before>
                <observers>
                    <udshipclass>
                        <type>singleton</type>
                        <class>udshipclass/observer</class>
                        <method>udropship_shipping_save_before</method>
                    </udshipclass>
                </observers>
            </udropship_shipping_save_before>
            <udropship_vendor_shipping_check_skipped>
                <observers>
                    <udshipclass>
                        <type>singleton</type>
                        <class>udshipclass/observer</class>
                        <method>udropship_vendor_shipping_check_skipped</method>
                    </udshipclass>
                </observers>
            </udropship_vendor_shipping_check_skipped>
        </events>

    </global>

    <frontend>
        <routers>
            <udshipclass>
                <use>standard</use>
                <args>
                    <module>ZolagoOs_OmniChannelShippingClass</module>
                    <frontName>udshipclass</frontName>
                </args>
            </udshipclass>
        </routers>
        <layout>
            <updates>
                <udshipclass>
                    <file>udshipclass.xml</file>
                </udshipclass>
            </updates>
        </layout>
        <events>
        </events>
    </frontend>

    <admin>
        <routers>
            <zosshipclassadmin>
                <use>admin</use>
                <args>
                    <module>ZolagoOs_OmniChannelShippingClass_Adminhtml</module>
                    <frontName>zosshipclassadmin</frontName>
                </args>
            </zosshipclassadmin>
        </routers>
    </admin>

    <adminhtml>
        <layout>
            <updates>
                <udshipclass>
                </udshipclass>
            </updates>
        </layout>
        <menu>
            <sales>
                <children>
                    <udropship>
                        <children>
                            <udshipclass_customer module="udshipclass">
                                <title>Customer Ship Classes</title>
                                <sort_order>200</sort_order>
                                <action>zosshipclassadmin/customer</action>
                            </udshipclass_customer>
                            <udshipclass_vendor module="udshipclass">
                                <title>Vendor Ship Classes</title>
                                <sort_order>205</sort_order>
                                <action>zosshipclassadmin/vendor</action>
                            </udshipclass_vendor>
                        </children>
                    </udropship>
                </children>
            </sales>
        </menu>
        <events>
            <udropship_adminhtml_shipping_edit_prepare_form>
                <observers>
                    <udshipclass>
                        <class>udshipclass/observer</class>
                        <method>udropship_adminhtml_shipping_edit_prepare_form</method>
                    </udshipclass>
                </observers>
            </udropship_adminhtml_shipping_edit_prepare_form>
            <udropship_adminhtml_vendor_edit_prepare_shipping_grid>
                <observers>
                    <udshipclass>
                        <class>udshipclass/observer</class>
                        <method>udropship_adminhtml_vendor_edit_prepare_shipping_grid</method>
                    </udshipclass>
                </observers>
            </udropship_adminhtml_vendor_edit_prepare_shipping_grid>
            <udropship_adminhtml_shipping_grid_prepare_columns>
                <observers>
                    <udshipclass>
                        <class>udshipclass/observer</class>
                        <method>udropship_adminhtml_shipping_grid_prepare_columns</method>
                    </udshipclass>
                </observers>
            </udropship_adminhtml_shipping_grid_prepare_columns>
            <udropship_adminhtml_shipping_grid_after_load>
                <observers>
                    <udshipclass>
                        <class>udshipclass/observer</class>
                        <method>udropship_adminhtml_shipping_grid_after_load</method>
                    </udshipclass>
                </observers>
            </udropship_adminhtml_shipping_grid_after_load>
        </events>
    </adminhtml>

    <default>
        <udropship>
	    </udropship>
    </default>

    <crontab>
        <jobs>
        </jobs>
    </crontab>
</config>
