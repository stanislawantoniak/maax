<?xml version="1.0"?>
<config>
    <modules>
        <GH_Regulation>
            <version>0.1.1</version>
        </GH_Regulation>
    </modules>
    <global>
        <models>
            <ghregulation>
                <class>GH_Regulation_Model</class>
                <resourceModel>ghregulation_resource</resourceModel>
            </ghregulation>
            <ghregulation_resource>
                <class>GH_Regulation_Model_Resource</class>
                <entities>
                    <regulation_kind>
                        <table>gh_regulation_kind</table>
                    </regulation_kind>
                    <regulation_type>
                        <table>gh_regulation_type</table>
                    </regulation_type>
                    <regulation_document>
                        <table>gh_regulation_document</table>
                    </regulation_document>
                    <regulation_document_vendor>
                        <table>gh_regulation_document_vendor</table>
                    </regulation_document_vendor>
                    <regulation_vendor_kind>
                        <table>gh_regulation_vendor_kind</table>
                    </regulation_vendor_kind>
                </entities>
            </ghregulation_resource>
        </models>
        <blocks>
            <ghregulation>
                <class>GH_Regulation_Block</class>
            </ghregulation>
        </blocks>
        <helpers>
            <ghregulation>
                <class>GH_Regulation_Helper</class>
            </ghregulation>
        </helpers>
        <resources>
            <ghregulation_setup>
                <setup>
                    <module>GH_Regulation</module>
                </setup>
            </ghregulation_setup>
        </resources>
    </global>
    <frontend>
        <routers>
            <ghregulation>
                <use>standard</use>
                <args>
                    <module>GH_Regulation</module>
                    <frontName>ghregulation</frontName>
                </args>
            </ghregulation>
        </routers>
        <translate>
            <modules>
                <GH_Regulation>
                    <files>
                        <default>GH_Regulation.csv</default>
                    </files>
                </GH_Regulation>
            </modules>
        </translate>
    </frontend>
</config>