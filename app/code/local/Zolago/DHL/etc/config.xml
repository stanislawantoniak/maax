<?xml version="1.0"?>
<config>
    <modules>
        <Zolago_DHL>
            <version>0.0.1</version>
        </Zolago_DHL>
    </modules>
    <global>
        <models>
            <zolagodhl>
                <class>Zolago_DHL_Model</class>
            </zolagodhl>
        </models>
        <helpers>
            <zolagodhl>
                <class>Zolago_DHL_Helper</class>
            </zolagodhl>
        </helpers>
        <resources>
            <zolagodhl_setup>
                <setup>
                    <module>Zolago_DHL</module>
                </setup>
            </zolagodhl_setup>
        </resources>
    </global>
    <frontend>
        <routers>
            <zolagodhl>
                <use>standard</use>
                <args>
                    <module>Zolago_DHL</module>
                    <frontName>zolagodhl</frontName>
                </args>
            </zolagodhl>
			<udropship>
				<args>
					<modules>
						<Zolago_DHL before="Unirgy_Dropship">Zolago_DHL_Dropship</Zolago_DHL>
					</modules>
				</args>
			</udropship>
        </routers>
        <layout>
            <updates>
                <zolagodhl>
                    <file>zolagodhl.xml</file>
                </zolagodhl>
            </updates>
        </layout>
    </frontend>
</config>
