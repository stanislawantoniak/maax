<?xml version="1.0"?>
<config>
    <modules>
        <Zolago_Help>
            <version>0.0.1</version>
        </Zolago_Help>
    </modules>
    <frontend>
        <routers>
            <help>
                <use>standard</use>
                <args>
                    <module>Zolago_Help</module>
                    <frontName>help</frontName>
                </args>
            </help>
        </routers>
        <translate>
            <modules>
                <Zolago_Help>
                    <files>
                        <default>Zolago_Help.csv</default>
                    </files>
                </Zolago_Help>
            </modules>
        </translate>
    </frontend>
</config>