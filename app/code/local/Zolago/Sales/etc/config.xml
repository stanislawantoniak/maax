<?xml version="1.0"?>
<config>
  <modules>
    <Zolago_Sales>
      <version>0.0.1</version>
    </Zolago_Sales>
  </modules>
  <global>
    <blocks>
      <zolagosales>
        <class>Zolago_Sales_Block</class>
      </zolagosales>
    </blocks>
    <models>
      <sales>
        <rewrite>
          <quote>Zolago_Sales_Model_Quote</quote>
          <order_config>Zolago_Sales_Model_Order_Config</order_config>
        </rewrite>
      </sales>
    </models>
    <helpers>
      <zolagosales>
        <class>Zolago_Sales_Helper</class>
      </zolagosales>
    </helpers>
  </global>
  <frontend>
    <translate>
      <modules>
        <Zolago_Sales>
          <files>
            <default>Zolago_Sales.csv</default>
          </files>
        </Zolago_Sales>
      </modules>
    </translate>
    <routers>
      <sales>
        <args>
          <modules>
            <Zolago_Sales before="Unirgy_Rma">Zolago_Sales</Zolago_Sales>
          </modules>
        </args>
      </sales>
    </routers>
  </frontend>
</config>
