<?xml version="1.0"?>
<config>
    <modules>
        <Zolago_Log>
            <version>0.0.1</version>
        </Zolago_Log>
    </modules>
    <global>
        <models>
            <zolagolog>
                <class>Zolago_Log_Model</class>
                <resourceModel>zolagolog_resource</resourceModel>
            </zolagolog>
            <zolagolog_resource>
                <class>Zolago_Log_Model_Resource</class>
            </zolagolog_resource>
        </models>
        <helpers>
            <zolagolog>
                <class>Zolago_Log_Helper</class>
            </zolagolog>
        </helpers>
    </global>
    <frontend>
        <translate>
            <modules>
                <zolagolog>
                    <files>
                        <default>Zolago_Log.csv</default>
                    </files>
                </zolagolog>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <zolagolog>
                    <files>
                        <default>Zolago_Log.csv</default>
                    </files>
                </zolagolog>
            </modules>
        </translate>
    </adminhtml>
</config>