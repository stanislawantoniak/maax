<?xml version="1.0"?>
<config>
	<modules>
		<Zolago_ManaPro_FilterAdmin>
			<version>0.0.1</version>
		</Zolago_ManaPro_FilterAdmin>
	</modules>
	<global>
		<helpers>
			<zolagomanaprofilteradmin>
				<class>Zolago_ManaPro_FilterAdmin_Helper</class>
			</zolagomanaprofilteradmin>
		</helpers>
		<blocks>
			<zolagomanaprofilteradmin>
				<class>Zolago_ManaPro_FilterAdmin_Block</class>
			</zolagomanaprofilteradmin>
			<manapro_filteradmin>
				<rewrite>
					<card_general>Zolago_ManaPro_FilterAdmin_Block_Card_General</card_general>
				</rewrite>
			</manapro_filteradmin>
		</blocks>
	</global>
	<admin>
		<routers>
			<adminhtml>
				<args>
					<modules>
						<zolagomanaprofilteradmin before="ManaPro_FilterAdmin">Zolago_ManaPro_FilterAdmin</zolagomanaprofilteradmin>
					</modules>
				</args>
			</adminhtml>
		</routers>
	</admin>
	<adminhtml>
		<translate>
			<modules>
				<zolagomanaprofilteradmin>
					<files>
						<default>Zolago_ManaPro_FilterAdmin.csv</default>
					</files>
				</zolagomanaprofilteradmin>
			</modules>
		</translate>
		<layout>
			<updates>
				<manapro_filtercolors>
					<file>zolago_manapro_filtercolors.xml</file>
				</manapro_filtercolors>
			</updates>
		</layout>
	</adminhtml>
</config>