<?xml version="1.0"?>
<config>
    <modules>
        <Zolago_Modago>
            <version>0.0.1</version>
        </Zolago_Modago>
    </modules>
	<frontend>
		<routers>
            <zolagomodago>
                <use>standard</use>
                <args>
                    <module>Zolago_Modago</module>
                    <frontName>modago</frontName>
                </args>
            </zolagomodago>
