		<Zolago_DropshipSplit>
			<version>0.0.1</version>
		</Zolago_DropshipSplit>
	</modules>
	<global>
		<blocks>
			<checkout>
				<rewrite>
					<cart>Zolago_DropshipSplit_Block_Cart</cart>
				</rewrite>
			</checkout>		
			<udsplit>
				<rewrite>
					<cart_vendor>Zolago_DropshipSplit_Block_Cart_Vendor</cart_vendor>
					<cart_review>Zolago_DropshipSplit_Block_Cart_Review</cart_review>
				</rewrite>
			</udsplit>		
		</blocks>
		<models>
			<udsplit>
				<rewrite>
					<carrier>Zolago_DropshipSplit_Model_Carrier</carrier>
				</rewrite>
			</udsplit>
		</models>
	</global>
