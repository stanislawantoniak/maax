<?xml version="1.0"?>
<config>
	<modules>
		<Zolago_Converter>
			<version>0.0.1</version>
		</Zolago_Converter>
	</modules>
	<global>
		<models>
			<zolagoconverter>
				<class>Zolago_Converter_Model</class>
				<resourceModel>zolagoconverter_resource</resourceModel>
			</zolagoconverter>
			<zolagoconverter_resource>
				<class>Zolago_Pos_Model_Resource</class>
				<entities>
				</entities>
			</zolagoconverter_resource>
		</models>
		<helpers>
			<zolagoconverter>
				<class>Zolago_Converter_Helper</class>
			</zolagoconverter>
		</helpers>
	</global>
	<adminhtml>
		<translate>
			<modules>
				<Zolago_Converter>
					<files>
						<default>Zolago_Converter.csv</default>
					</files>
				</Zolago_Converter>
			</modules>
		</translate>
	</adminhtml>
	<default>
		<zolagoconverter>
			<config>
				<location></location>
				<uri></uri>
				<apikey></apikey>
				<login></login>
				<password></password>
			</config>
		</zolagoconverter>
	</default>
</config>
