<?xml version="1.0"?>
<config>
    <modules>
        <Zolago_CatalogInventory>
            <version>0.1.0</version>
        </Zolago_CatalogInventory>
    </modules>
    <global>
        <models>
            <zolago_cataloginventory>
                <class>Zolago_CatalogInventory_Model</class>
                <resourceModel>zolago_cataloginventory_resource</resourceModel>
            </zolago_cataloginventory>
            <zolago_cataloginventory_resource>
                <class>Zolago_CatalogInventory_Model_Resource</class>
            </zolago_cataloginventory_resource>
            <cataloginventory_resource>
                <rewrite>
                    <stock_status>Zolago_CatalogInventory_Model_Resource_Stock_Status</stock_status>
                </rewrite>
            </cataloginventory_resource>
            <cataloginventory>
                <rewrite>
                    <stock_item>Zolago_CatalogInventory_Model_Stock_Item</stock_item>
                </rewrite>
            </cataloginventory>
        </models>
        <helpers>
            <zolagocataloginventory>
                <class>Zolago_CatalogInventory_Helper</class>
            </zolagocataloginventory>
        </helpers>
    </global>
</config>