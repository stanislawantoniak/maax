<div class="col-md-12">

    <?php $productBlogItems = $this->_productBlogItems; ?>

    <?php if ($productBlogItems->getSize() > 0): ?>
        <?php foreach ($productBlogItems as $productBlogItem): ?>
            <div class="col-md-12">
                <a href="<?php echo $productBlogItem->getUrl(); ?>">
                    <img width="100" height="100"
                         src="<?php echo Mage::getBaseUrl(Mage_Core_Model_Store::URL_TYPE_MEDIA) . $productBlogItem->getData("post_thumbnail"); ?>"/>
                    <?php echo $productBlogItem->getTitle(); ?>
                </a>
            </div>
            <?php //Zend_Debug::dump($productBlogItem->getData("post_thumbnail")); ?>
        <?php endforeach; ?>
    <?php endif; ?>
</div>