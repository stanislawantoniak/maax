    elFinder.prototype.commands.rm = function() {

        this.shortcuts = [{
            pattern     : 'delete ctrl+backspace'
        }];

        this.getstate = function(sel) {
            var fm = this.fm;
            sel = sel || fm.selected();
            return !this._disabled && sel.length && jQuery.map(sel, function(h) { var f = fm.file(h); return f && f.phash && !f.locked ? h : null }).length == sel.length
                ? 0 : -1;
        }

        this.exec = function(hashes) {
            var self   = this,
                fm     = this.fm,
                dfrd   = jQuery.Deferred()
                    .fail(function(error) {
                        error && fm.error(error);
                    }),
                files  = this.files(hashes),
                cnt    = files.length,
                cwd    = fm.cwd().hash,
                goroot = false;

            if (!cnt || this._disabled) {
                return dfrd.reject();
            }

            jQuery.each(files, function(i, file) {
                if (!file.phash) {
                    return !dfrd.reject(['errRm', file.name, 'errPerm']);
                }
                if (file.locked) {
                    return !dfrd.reject(['errLocked', file.name]);
                }
                if (file.hash == cwd) {
                    goroot = fm.root(file.hash);
                }
            });

            if (dfrd.state() == 'pending') {
                files = this.hashes(hashes);

                fm.confirm({
                    title  : self.title,
                    text   : 'confirmRm',
                    accept : {
                        label    : 'btnRm',
                        callback: function () {
                            fm.lockfiles({files: files});
                            fm.request({
                                data: {cmd: 'rm', targets: files},
                                notify: {type: 'rm', cnt: cnt},
                                preventFail: true
                            })
                                .fail(function (error) {
                                    dfrd.reject(error);
                                })
                                .done(function (data) {
                                    dfrd.done(data);
                                    goroot && fm.exec('open', goroot)
                                }
                            ).always(function () {
                                    fm.unlockfiles({files: files});
                                });
                            jQuery('#delete_button').attr('disabled', false);
                        }
                    },
                    cancel : {
                        label    : 'btnCancel',
                        callback : function() { dfrd.reject();jQuery('#delete_button').attr('disabled',false); }
                    }
                });
            }

            return dfrd;
        }

    }
