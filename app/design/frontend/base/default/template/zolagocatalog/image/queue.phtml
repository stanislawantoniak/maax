                                    var n = parseInt(jQuery("#dialog_by_csv_result #map_csv_valid").text());
                                    jQuery("#dialog_by_csv_result #map_csv_valid").text(n + parseInt(response.message.count));

                                    if (percentageOfDone >= 100) {
                                        showCSVResultModal(pidList);
                                    } else {
                                        jQuery(".progress-bar").css({"width": percentageOfDone + "%"}).text(Math.round(percentageOfDone) + " %");
                                    }
