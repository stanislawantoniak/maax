                type: 'error',
                timeout: 2000
            });
            return false;
        }
        jQuery("#dialog_by_name").modal('hide');
        startLoading();
        //map by name
        var FilesToMap = [];
        var validFiles = jQuery("#dialog_by_name").data("validfiles");

        jQuery.each(validFiles, function (i, fileNames) {
            jQuery.each(fileNames, function (i, fileName) {
                FilesToMap.push(fileName);
            })
        })
        var chunks = [];
        if (FilesToMap.length <= mapByNameBatchSize) {
            chunks.push(FilesToMap);
        } else {
            chunks = array_chunk(FilesToMap, mapByNameBatchSize);
        }

        var percentageOfDone = 0;
        var pidList = [];
        jQuery(chunks).each(function (n, chunk) {
            //make ajax mapping
            jQuery.ajaxQueue({
                url: '<?php echo Mage::getUrl('udprod/vendor_ajax/mapByName',array('_forced_secure'=>true)); ?>',
                data: {data: chunk},
                type: "POST",
                success: function (response) {
