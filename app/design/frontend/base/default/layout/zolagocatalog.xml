<?xml version="1.0"?>
<layout version="0.1.0">
	<udprod_vendor_mass_index>
		<update handle="udropship_vendor" />
		<update handle="formkey" />
		<update handle="adminhtml_head" />
		
		<reference name="content">
			<block type="core/html_calendar" name="head.udropship.calendar" as="udropship.calendar" template="unirgy/dropship/calendar.phtml"></block>
            
			<block name="mass" type="zolagocatalog/vendor_mass" template="zolagocatalog/mass.phtml">
				<block type="zolagocatalog/vendor_mass_attributesetswitcher" name="attribute_set_switcher" as="attribute_set_switcher" template="zolagocatalog/mass/attributesetswitcher.phtml"/>
				<block type="zolagocatalog/vendor_mass_staticfilter" name="static_filter" as="static_filter" template="zolagocatalog/mass/staticfilter.phtml"/>
				<block type="zolagocatalog/vendor_mass_editor" name="editor" as="editor" template="zolagocatalog/mass/editor.phtml"/>
			</block>
		</reference>
	</udprod_vendor_mass_index>
	<udprod_vendor_image_queue>
		<update handle="udropship_vendor" />
		<update handle="bootstrap" />
		<reference name="head">
		    <action method="addItem"><type>skin_js</type><name>plugins/plupload/jquery-ui/js/jquery-ui-1.10.4.min.js</name></action>
		    <action method="addItem"><type>skin_js</type><name>plugins/plupload/plupload.full.min.js</name></action> 
		    <action method="addItem"><type>skin_js</type><name>plugins/plupload/jquery.ui.plupload/jquery.ui.plupload.min.js</name></action>
		    <action method="addCss"><stylesheet>plugins/plupload/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.css</stylesheet></action>
		    <action method="addCss"><stylesheet>plugins/plupload/jquery.ui.plupload/css/jquery.ui.plupload.css</stylesheet></action>
		</reference>
		<reference name="content">
			<block name="image" type="zolagocatalog/vendor_image" template="zolagocatalog/image.phtml">			    
            </block>		    
		</reference>
	</udprod_vendor_image_queue>
	<udprod_vendor_image_index>
<!--		<update handle="udropship_vendor" />
		<update handle="formkey" />
		<update handle="adminhtml_head" />
-->     <remove name="dashboard">		
		<update handle="bootstrap" />
		<reference name="head">
		    <action method="addItem"><type>skin_js</type><name>plugins/plupload/plupload.full.min.js</name></action>
		    <action method="addItem"><type>skin_js</type><name>plugins/elfinder/js/elfinder.full.js</name></action>
		    <action method="addItem"><type>skin_js</type><name>plugins/elfinder/js/jquery-ui.min.js</name></action>
		    <action method="addItem"><type>skin_js</type><name>plugins/plupload/jquery.plupload.queue/jquery.plupload.queue.js</name></action>
		    <action method="addCss"><stylesheet>plugins/elfinder/css/jquery-ui.css</stylesheet></action>
		    <action method="addCss"><stylesheet>plugins/plupload/jquery.plupload.queue/css/jquery.plupload.queue.css</stylesheet></action>
		    <action method="addCss"><stylesheet>plugins/elfinder/css/elfinder.full.css</stylesheet></action>
		    <action method="addCss"><stylesheet>plugins/elfinder/css/theme.css</stylesheet></action>
		</reference>
		<reference name="content">
			<block name="image" type="zolagocatalog/vendor_image" template="zolagocatalog/image.phtml">			    
			    <block name="image_queue" type="zolagocatalog/vendor_image_queue" template="zolagocatalog/image/queue.phtml"/>
			    <block name="image_list" type="zolagocatalog/vendor_image_list" template="zolagocatalog/image/list.phtml"/>
			</block>
		</reference>
	</udprod_vendor_image_index>
</layout>
